{
  "name": "@jiango/slp-gameplans-hud",
  "productName": "slp-gameplans-hud",
  "version": "0.0.1",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "watch": "tsc -w",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts . --fix && npx prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshjiang/slp-gameplan-hud.git"
  },
  "keywords": [
    "slippi",
    "melee"
  ],
  "author": "Josh Jiang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshjiang/slp-gameplan-hud/issues"
  },
  "homepage": "https://github.com/joshjiang/slp-gameplan-hud#readme",
  "private": true,
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "slp_gameplans"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.54",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/bootstrap": "^5.0.1",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.6",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "css-loader": "^4.3.0",
    "eslint": "^7.24.0",
    "electron": "12.0.4",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^5.2.1",
    "node-loader": "^1.0.3",
    "prettier": "2.2.1",
    "style-loader": "^1.3.0",
    "ts-loader": "^8.1.0",
    "typescript": "^4.2.4",
    "uuid": "^8.3.2"
  },
  "dependencies": {
    "@slippi/slippi-js": "^6.0.1",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.2.2",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.19.6",
    "@types/react": "^16.14.1",
    "@types/react-bootstrap": "^0.32.25",
    "@types/react-dom": "^16.9.10",
    "@types/react-router": "^5.1.8",
    "@vinceau/slp-realtime": "3.1.0",
    "bootstrap": "^4.5.3",
    "electron-squirrel-startup": "^1.0.0",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.0",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "rxjs": "^6.6.7",
    "typescript": "^4.1.2",
    "web-vitals": "^0.2.4"
  }
}
